---
layout: 
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
  {% unless page.url contains '.xml' or page.url contains 'assets' or page.url contains '/page/' %}
      <url>
        <loc>{{ site.url }}{{ site.baseurl }}{{ page.url }}</loc>
        <lastmod>{{ page.last_modified_at | default: site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>{% if page.url == '/' %}1.0{% else %}0.8{% endif %}</priority>
       </url>
  {% endunless %}
  {% endfor %}
  {% for post in site.posts %}
      <url>
        <loc>{{ site.url }}{{ site.baseurl }}{{ post.url | replace: 'index.html', '' }}</loc>
        <lastmod>{{ post.last_modified_at | default: post.date | date_to_xmlschema }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.9</priority>
       </url>
  {% endfor %}
  {% for page in site.without-plugin %}
      <url>
        <loc>{{ site.url }}{{ site.baseurl }}{{ page.url | replace: 'index.html', '' }}</loc>
        <lastmod>{{ page.last_modified_at | default: site.time | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.7</priority>
       </url>
  {% endfor %}
</urlset>